{
  "name": "@heartsradiant/blocks",
  "version": "4.2.0",
  "description": "REST API framework for Node.js",
  "homepage": "https://github.com/heartsradiant/blocks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heartsradiant/blocks.git"
  },
  "bugs": {
    "url": "https://github.com/heartsradiant/blocks/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "keywords": [
    "nodejs",
    "api",
    "server",
    "framework"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check -u",
    "----BUNDLE": "",
    "build": "babel src --out-dir dist --source-maps",
    "----LINT": "",
    "lint:js": "eslint --quiet --ext .js src/",
    "lint:js-fix": "eslint --quiet --fix --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "start": "nodemon examples/simple.js",
    "test": "node tests/index.js | tap-nirvana",
    "tdd": "nodemon --watch src --watch tests --watch examples --exec \"npm test\"",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@asd14/m": "^6.6.0",
    "@asd14/pluginus": "^2.4.0",
    "@sindresorhus/slugify": "^1.1.0",
    "accepts": "^1.3.7",
    "ajv": "^7.1.1",
    "ajv-formats": "^1.5.1",
    "ajv-keywords": "^4.0.0",
    "busboy": "^0.3.1",
    "connect": "^3.7.0",
    "content-type": "^1.0.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "^4.3.1",
    "helmet": "^4.4.1",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.9.6"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^6.12.0",
    "@asd14/fetch-node": "^1.2.2",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "coveralls": "^3.1.0",
    "eslint": "^8.6.0",
    "jsonwebtoken": "^8.5.1",
    "markdownlint-cli": "^0.30.0",
    "nodemon": "^2.0.7",
    "npm-check": "^5.9.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "riteway": "^6.3.1",
    "semantic-release": "^17.3.9",
    "tap-nirvana": "^1.1.0"
  }
}
