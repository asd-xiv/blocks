{
  "name": "@asd14/blocks",
  "version": "0.0.1",
  "description": "REST API framework for Node.js",
  "homepage": "https://github.com/asd-xiv/blocks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asd-xiv/blocks.git"
  },
  "bugs": {
    "url": "https://github.com/asd-xiv/blocks/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "main": "src/index.js",
  "keywords": [
    "nodejs",
    "api",
    "server",
    "framework"
  ],
  "scripts": {
    "----UTIL": "",
    "prepare": "git config --local core.hooksPath .githooks",
    "setup": "npm ci --prefer-offline --no-audit --progress=false",
    "update": "ncu --interactive",
    "postupdate": "npm audit fix",
    "format": "prettier --write --prose-wrap=always . && eslint --fix .",
    "----LINT": "",
    "lint:js": "eslint --quiet --ext .js src/",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:md && npm run lint:js",
    "----TEST": "",
    "start": "nodemon examples/simple.js",
    "test": "node tests/index.js | tap-nirvana",
    "tdd": "nodemon --watch src --watch tests --watch examples --exec \"npm test\"",
    "coverage:report": "nyc npm test && nyc report --reporter=html",
    "coverage:ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "@asd14/m": "^7.8.1",
    "@asd14/pluginus": "^3.0.0",
    "@sindresorhus/slugify": "^2.1.0",
    "accepts": "^1.3.8",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "busboy": "^1.6.0",
    "connect": "^3.7.0",
    "content-type": "^1.0.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "debug": "^4.3.4",
    "helmet": "^5.0.2",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.3",
    "rambda": "^7.0.3"
  },
  "devDependencies": {
    "@asd14/eslint-config": "^7.3.0",
    "@asd14/fetch-node": "^1.2.2",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.177",
    "@types/busboy": "^1.5.0",
    "@types/connect": "^3.4.35",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.14.0",
    "jsonwebtoken": "^8.5.1",
    "markdownlint-cli": "^0.31.1",
    "nodemon": "^2.0.16",
    "npm-check-updates": "^12.5.11",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "riteway": "^6.3.1",
    "semantic-release": "^19.0.2",
    "tap-nirvana": "^1.1.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0",
    "npm": ">=8"
  }
}
